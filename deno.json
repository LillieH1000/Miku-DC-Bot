{
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "strict": true
  },
  "imports": {
    "@date-fns/tz": "npm:@date-fns/tz@^1.4.1",
    "@discordjs/opus": "npm:@discordjs/opus@^0.10.0",
    "@discordjs/voice": "npm:@discordjs/voice@^0.19.0",
    "@snazzah/davey": "npm:@snazzah/davey@^0.1.9",
    "date-fns": "npm:date-fns@^4.1.0",
    "discord.js": "npm:discord.js@^14.25.1",
    "lz-string": "npm:lz-string@^1.5.0",
    "sodium-native": "npm:sodium-native@^5.0.10"
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "nodeModulesDir": "auto",
  "permissions": {
    "default": {
      "env": true,
      "net": true,
      "read": [
        "commands",
        "globals",
        "node_modules",
        "utils"
      ],
      "run": [
        "ffmpeg",
        "yt-dlp"
      ]
    },
    "register": {
      "env": true,
      "net": [
        "*.discord.com"
      ],
      "read": [
        "commands"
      ]
    }
  }
}
